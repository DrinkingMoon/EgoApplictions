<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
  </configSections>
  <unity>
    <containers>
      <container>
        <register type="Ego.Domain.IRepositoryContext,Ego.Domain"
                  mapTo="Ego.Domain.Repositories.EntityFramework.EntityFrameworkRepositoryContext, 
                  Ego.Domain.Repositories.EntityFramework" />
        <register type="Ego.Domain.Repositories.IInventoryRepository,Ego.Domain.Repositories" 
                  mapTo="Ego.Domain.Repositories.EntityFramework.InventoryRepository, Ego.Domain.Repositories.EntityFramework" />
        <register type="Ego.Domain.Repositories.IProductRepository,Ego.Domain.Repositories"
                  mapTo="Ego.Domain.Repositories.EntityFramework.ProductRepository, Ego.Domain.Repositories.EntityFramework" />
        <register type="Ego.Domain.Repositories.IStorageRepository,Ego.Domain.Repositories"
                  mapTo="Ego.Domain.Repositories.EntityFramework.StorageRepository, Ego.Domain.Repositories.EntityFramework" />
      </container>
    </containers>
  </unity>
</configuration>