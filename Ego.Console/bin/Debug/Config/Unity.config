<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
  </configSections>
  <unity>
    <containers>
      <container>
	  <!--DbContext-->
        <register type="Ego.Domain.IContext,Ego.Domain" mapTo="Ego.Domain.Repositories.EFDbContextEgo.ContextEntityFramework, 
                  Ego.Domain.Repositories.EFDbContextEgo" name ="EFDbContextEgo"/>
        <register type="Ego.Domain.IContext,Ego.Domain" mapTo="Ego.Domain.Repositories.EFDbContextUser.ContextEntityFramework, 
                  Ego.Domain.Repositories.EFDbContextUser" name ="EFDbContextUser"/>
				  
	  <!--Repositories-->
        <register type="Ego.Domain.Repositories.IRepInventory,Ego.Domain" 
                  mapTo="Ego.Domain.Repositories.EntityFramework.RepInventory, Ego.Domain.Repositories" />
        <register type="Ego.Domain.Repositories.IRepProduct,Ego.Domain"
                  mapTo="Ego.Domain.Repositories.EntityFramework.RepProduct, Ego.Domain.Repositories" />
        <register type="Ego.Domain.Repositories.IRepStorage,Ego.Domain"
                  mapTo="Ego.Domain.Repositories.EntityFramework.RepStorage, Ego.Domain.Repositories" />
        <register type="Ego.Domain.Repositories.IRepStockInBill,Ego.Domain"
                  mapTo="Ego.Domain.Repositories.EntityFramework.RepStockInBill, Ego.Domain.Repositories" />
        <register type="Ego.Domain.Repositories.IRepUser,Ego.Domain"
                  mapTo="Ego.Domain.Repositories.EntityFramework.RepUser, Ego.Domain.Repositories" />
				  
	  <!--Services-->
        <!--<register type="Ego.Domain.Service.IServiceDomain,Ego.Domain"
                  mapTo="Ego.Domain.Service.ServiceDomain,Ego.Domain" />-->
				  
	  <!--Events-->
		    <!--<register type="Ego.Domain.Events.Interface.IEventStockInBill,Ego.Domain"
                  mapTo="Ego.Domain.Events.EventStockInBill,Ego.Domain" />
        <register type="Ego.Domain.Events.Interface.IEventStorage,Ego.Domain"
                  mapTo="Ego.Domain.Events.EventStorage,Ego.Domain" />-->
      </container>
    </containers>
  </unity>
</configuration>